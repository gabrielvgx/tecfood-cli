{
    "SERVICES": {
        "BIRT": {
            "REGISTRY": "dockerhub.teknisa.com",
            "IMAGE": "birt",
            "TAG": "latest"
        },
        "BASEDEV": {
            "REGISTRY": "dockerhub.teknisa.com",
            "IMAGE": "c9-php7",
            "TAG": "latest"
        },
        "APP": {
            "IMAGE": "app-image"
        }
    },
    
    "BIRT_BASEPATH": "<>HOME_PATH<>/workfolder",
    "BIRT_EXT_PORT_9191": "9191",
