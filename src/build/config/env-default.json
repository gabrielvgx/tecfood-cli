    "BIRT_CONTAINER_NAME": "birt",

    "BASEDEV_CONTAINER_NAME": "basedev-container",
    "BASEDEV_BASEPATH": "<>HOME_PATH<>/workfolder",
    "BASEDEV_SSHPATH": "<>HOME_PATH<>/.ssh",
    "BASEDEV_EXT_PORT_80": "3000",

    "APP_CONTAINER_NAME": "app-container"
}