{
    "SERVICES": {
        "BIRT": {
            "REGISTRY": "dockerhub.teknisa.com",
            "IMAGE": "birt",
            "TAG": "latest"
        },
        "BASEDEV": {
            "REGISTRY": "dockerhub.teknisa.com",
            "IMAGE": "c9-php7",
            "TAG": "latest"
        },
        "APP": {
            "IMAGE": "app-image"
        },
        "MONGO": {
            "IMAGE": "mongo:3.2"
        },
        "ALGORITMO": {
            "IMAGE": "node:10"
        }
    },
    
    "BIRT_BASEPATH": "<>HOME_PATH<>/workfolder",
    "BIRT_EXT_PORT_9191": "9191",
    "BIRT_EXT_PORT_8080": "8080",
    "BIRT_CONTAINER_NAME": "birt",

    "BASEDEV_CONTAINER_NAME": "basedev-container",
    "BASEDEV_BASEPATH": "<>HOME_PATH<>/workfolder",
    "BASEDEV_SSHPATH": "<>HOME_PATH<>/.ssh",
    "BASEDEV_EXT_PORT_80": "3000",

    "APP_CONTAINER_NAME": "app-container",
    "APP_BASE_PATH": "<>HOME_PATH<>/workfolder",
    "APP_SSHPATH": "<>HOME_PATH<>/.ssh",

    "MONGO_CONTAINER_NAME": "mongo-container",
    "MONGO_EXT_PORT_27017": "27017",

    "ALGORITMO_CONTAINER_NAME": "algoritmo-container",
    "ALGORITMO_BASEPATH": "<>HOME_PATH<>/workfolder/algoritmo",
    "ALGORITMO_EXT_PORT_9191": "9521"
}